<%= form_for @post do |f| %>
	<%= image_tag("red-mush.png", :alt => "achievements", :class => "homeFooterIcon") %>
	<%= f.label :achievements %><br>
	<%= f.text_field :achievements, class: 'form-control' %><br>
	<br>
	<%= image_tag("poison-mush.png", :alt => "obstables", :class => "homeFooterIcon") %>
	<%= f.label :obstacles %><br>
	<%= f.text_area :obstacles, class: 'form-control' %><br>
	<br>
	<%= image_tag("super-star.png", :alt => "goals", :class => "homeFooterIcon") %>
	<%= f.label :goals %><br>
	<%= f.text_area :goals, class: 'form-control' %><br>
	<br>
	<%= image_tag("block.png", :alt => "panic score", :class => "homeFooterIcon") %>
	<%= f.label :score %><br>
	<%= f.text_area :score, class: 'form-control' %><br>
	<br>
	<%= f.submit class: 'button center' %>
<% end %>
